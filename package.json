{
  "name": "wikitree-browser-extension",
  "version": "1.2.0",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js  --env browser=chrome --watch",
    "build": "webpack --config webpack/webpack.prod.js --env browser=chrome",
    "build-dev": "webpack --config webpack/webpack.dev.js --env browser=chrome",
    "watch-firefox": "webpack --config webpack/webpack.dev.js  --env browser=firefox --watch",
    "build-firefox": "webpack --config webpack/webpack.prod.js --env browser=firefox",
    "build-and-run-firefox-windows": "npm run build-firefox-dev && \"C:\\Program Files\\Mozilla Firefox\\firefox.exe\" \"about:debugging#/runtime/this-firefox\"",
    "build-firefox-dev": "webpack --config webpack/webpack.dev.js --env browser=firefox",
    "test": "jest",
    "clean": "rimraf dist",
    "download-templates": "node download-templates.js",
    "update-test-pages": "node src/testing/update_test_pages.js",
    "zip": "npm run download-templates && npm run clean && npm run build && cd dist && bestzip ../dist-chrome.zip *",
    "zip-firefox": "npm run download-templates && npm run clean && npm run build-firefox && cd dist && bestzip ../dist-firefox.zip *",
    "zip-and-rename-both": "npm run zip && npm run zip-firefox && python scripts/name_zip.py"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikitree/wikitree-browser-extension.git"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "axios": "^1.4.0",
    "cookiejs": "^2.1.2",
    "datatables.net-dt": "^1.13.5",
    "date-fns": "^2.30.0",
    "diff": "^7.0.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.13.3",
    "jquery-ui-dist": "^1.13.3",
    "jquery-ui-touch-punch": "^0.2.3",
    "js-cookie": "^3.0.1",
    "jszip": "^3.10.1",
    "path-browserify": "^1.0.1",
    "sheetjs": "^2.0.0",
    "url": "^0.11.2",
    "util": "^0.12.5",
    "wikitree-js": "^0.4.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "bestzip": "^2.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.11.0",
    "eslint": "^8.35.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "prettier": "2.7.1",
    "prompt": "^1.3.0",
    "rimraf": "^3.0.2 ",
    "sinon-chrome": "^3.0.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.0.0",
    "webpack-target-webextension": "^1.0.4"
  }
}
